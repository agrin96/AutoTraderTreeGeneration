{
    "save_best":true,
    "process_pool_size":10,
    "generations": 300,
    "training":{
        "traintest_split":0.66,
        "train_sampling_split":0.10,
        "rotation_interval":2,
        "rotate_training_data":true,
        "training_sets":2
    },
    "population":{
        "initial_population":300,
        "max_population":300,
        "initial_buy_node_depth":2,
        "max_buy_node_depth":15,
        "initial_sell_node_depth":3,
        "max_sell_node_depth":15,
        "unique_tree_variables":false
    },
    "mutation":{
        "mutation_rate":0.75,
        "threshold_step_percent":0.75,
        "threshold_step_interval":10,
        "mutation_types": {
            "replace":0.50,
            "insert_node":1.0,
            "insert_terminal":0.25,
            "step_threshold":0.90
        },
        "alphas":10
    },
    "evaluation":{
        "initial_funds":100,
        "trading_fee_percent":0.001
    },
    "selection":{
        "tournament_size":10,
        "survivors_percent": 0.25,
        "search_distance_modifier":0.25
    },
    "crossover":{
        "crossover_rate":0.75
    },
    "data_file_path":"../first_week_indicator_data.csv",
    "variables":[
        "best_ask",
        "best_bid",
        "volume",
        "atr",
        "atr_sma",
        "bop",
        "bop_sma",
        "bop_prime",
        "adl",
        "ulcer",
        "ulcer_normal",
        "ulcer_diff",
        "stochastic",
        "stochastic_signal",
        "stochastic_diff",
        "rsi",
        "rsi_oversold",
        "rsi_overbought",
        "macd_4_9_3",
        "macd_4_9_3_signal",
        "macd_12_26_9",
        "macd_12_26_9_signal",
        "bollinger_upper",
        "bollinger_lower",
        "percent_b",
        "obv",
        "kama_short",
        "kama_mid",
        "kama_long"],
    "buy_terminals":["BUY","HOLD"],
    "sell_terminals":["SELL","HOLD"]
}